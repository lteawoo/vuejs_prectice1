rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // match /{document=**} {
    //   allow read, write;
    // }
    match /users/{userId} {
      allow read, update: if request.auth != null && userId == request.auth.uid;
      allow create, delete: if false; // functions
    }
  }
}